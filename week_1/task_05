#include <iostream>
using namespace std;

int main() {
    int n, m;
    cin >> n >> m;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            if ((1 <= i) and (i <= n - 2) and (1 <= j) and (j <= m - 2))
                cout << " ";
            else
                cout << "+";
        }
        cout << '\n';
    }
}
