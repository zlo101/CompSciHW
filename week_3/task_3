#include <iostream>
using namespace std;

int main() {
    int n, a[11][11];

    cin >> n;
    for (int i = 0; i < n; ++i)
        for (int j = 0; j < n; ++j)
            cin >> a[i][j];

    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < n; ++j)
            cout << a[j][i] << ' ';
        cout << '\n';
    }
}
